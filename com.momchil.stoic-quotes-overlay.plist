<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
 <dict>
   <key>Label</key>
   <string>com.momchil.stoic-quotes-overlay</string>

   <key>ProgramArguments</key>
   <array>
     <string>~/.bin/stoic_qoutes_overlay</string>
   </array>

   <key>RunAtLoad</key>
   <true/>

   <key>KeepAlive</key>
   <false/>

   <key>StandardOutPath</key>
   <string>/tmp/stoic-quotes-overlay.log</string>

   <key>StandardErrorPath</key>
   <string>/tmp/stoic-quotes-overlay.err</string>
 </dict>
</plist>
